{
  "expo": {
    "name": "DelipuCash",
    "slug": "DelipuCash",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/logo.png",
    "scheme": "delipucash",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidStatusBar": {
      "barStyle": "dark-content"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arolainc.DelipuCash",
      "buildNumber": "1",
      "associatedDomains": [
        "applinks:delipucashserver.vercel.app"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "DelipuCash needs camera access to record videos and take photos for surveys and content creation.",
        "NSMicrophoneUsageDescription": "DelipuCash needs microphone access to record audio for video content.",
        "NSPhotoLibraryUsageDescription": "DelipuCash needs photo library access to upload images and videos.",
        "NSPhotoLibraryAddUsageDescription": "DelipuCash needs permission to save your recorded videos and photos to your device.",
        "NSLocationWhenInUseUsageDescription": "DelipuCash uses your location to show relevant surveys and content in your area."
      }
    },
    "android": {
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "blockedPermissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "package": "com.arolainc.DelipuCash",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "delipucashserver.vercel.app",
              "pathPrefix": "/reset-password"
            },
            {
              "scheme": "https",
              "host": "https://delipucash-latest.vercel.app",
              "pathPrefix": "/video"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "./plugins/withDisableLint.js",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 136,
          "resizeMode": "contain",
          "backgroundColor": "#0f0f23",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#0f0f23"
          }
        }
      ],
      "expo-audio",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "1.9.24",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "extraProguardRules": "-dontwarn **"
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "DelipuCash uses your location to show relevant surveys and content in your area.",
          "locationWhenInUsePermission": "DelipuCash uses your location to show relevant surveys and content in your area."
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "DelipuCash needs camera access to record videos and take photos for surveys and content creation.",
          "microphonePermission": "DelipuCash needs microphone access to record audio for video content.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-document-picker",
        {
          "permission": "DelipuCash needs access to your files to upload documents and media."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "DelipuCash needs access to your photos to upload images and videos for surveys and content.",
          "cameraPermission": "DelipuCash needs camera access to take photos for surveys and content creation."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "DelipuCash needs access to your photo library to save and upload videos and images.",
          "savePhotosPermission": "DelipuCash needs permission to save your recorded videos to your device.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-screen-orientation",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3d907cc1-a01f-435d-b9f4-7cfd7d9ef7c5"
      }
    },
    "owner": "landwind"
  }
}
