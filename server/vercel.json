{
  "version": 2,
  "name": "node-js",
  "installCommand": "bun install",
  "buildCommand": "bun run vercel-build",
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 60
    }
  }
}