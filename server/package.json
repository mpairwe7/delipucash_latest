{
  "name": "delipucash-server",
  "version": "1.0.0",
  "main": "api/index.js",
  "license": "MIT",
  "type": "module",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bun --watch index.js",
    "start": "bun index.js",
    "build": "prisma generate",
    "clean": "rm -rf node_modules/.cache dist .turbo",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma migrate deploy",
    "db:push": "bun prisma db push",
    "db:studio": "bun prisma studio",
    "db:reset": "bun prisma migrate reset",
    "db:seed": "bun prisma/seed.mjs",
    "seed:mock": "bun scripts/seed-mock-data.mjs",
    "lint": "bun run --bun eslint .",
    "test": "bun test"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@prisma/extension-accelerate": "^3.0.1",
    "@types/node": "^25.2.3",
    "axios": "^1.13.5",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dayjs": "^1.11.19",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-async-handler": "^1.2.0",
    "form-data": "^4.0.5",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^6.21.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "pg": "8.17.2",
    "postgres": "^3.4.8",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prisma": "^7.4.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.8",
  "private": true
}
